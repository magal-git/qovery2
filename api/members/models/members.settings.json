{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "name": "Members"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "email",
      "default": "email@example.com",
      "unique": true,
      "required": true
    }
  }
}
